    "graphql-yoga": "^1.18.3",
    "heroku": "^7.39.2",
    "merge-graphql-schemas": "^1.7.7",
    "morgan": "^1.10.0",
    "prisma": "^1.34.10",
    "prisma-client-lib": "^1.34.10"
  }
